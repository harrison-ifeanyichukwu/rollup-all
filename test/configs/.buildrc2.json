{
    "srcDir": "src",

    "mainModuleFileName": "main.js",

    "mainModuleName": "RollupAll",

    "fileExtensions": [".js"],

    "exclude": [],

    "include": ["*"],

    "externalModules": ["fs", "path"],

    "copyAssets": false,

    "uglify": false,

    "interop": true,

    "sourcemap": true,

    "distConfig": {
        "enabled": true,
        "format": "iife",
        "outDir": "dist",
        "uglify": true,
        "interop": false,
        "sourcemap": false,
        "copyAssets": false,
        "exclude": ["main.js"],
        "include": ["src/**/*.js"],
        "globals": {},
        "watch": {}
    },

    "libConfig": {
        "enabled": true,
        "format": "cjs",
        "outDir": "lib",
        "uglify": true,
        "interop": false,
        "sourcemap": false,
        "copyAssets": false,
        "exclude": ["main.js"],
        "include": ["src/**/*.js"],
        "globals": {},
        "watch": {}
    }
}