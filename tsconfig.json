{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ES5",
    "module": "commonjs",
    "esModuleInterop": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "noImplicitAny": false,
    "downlevelIteration": true,
    "outDir": "./build",
    "lib": ["dom", "es5", "es2015.collection", "es2015.iterable"]
  },
  "include": ["src"]
}
